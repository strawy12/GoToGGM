    - storyName: "\uC9C0\uAC01\uC744 \uAE68\uB2EB\uACE0 \uACE0\uBBFC\uC911\uC778
        \uC815\uBC30"
      mainStory: "\uB208\uC744 \uB72C \uB2F9\uC2E0\uC740 \uAE30\uC219\uC0AC\uC5D0
        \uAC00\uC57C\uD55C\uB2E4\uB294 \uAC83\uC744 \uAE68\uB2EC\uACE0 \uD604\uC7AC
        \uC2DC\uAC04\uC744 \uBCF4\uC558\uB2E4\uAC00 \uB2F9\uC2E0\uC740 \uACBD\uC545\uC744
        \uAE08\uCE58 \uBABB\uD558\uC600\uC2B5\uB2C8\uB2E4.\n\n\uC2DC\uACC4\uC758
        \uBD84\uCE68\uACFC \uC2DC\uCE68\uC740 \uB9C8\uCE58 \uD604\uC7AC\uC758 \uB0B4
        \uAE30\uBD84\uC744 \uD45C\uD604\uD574\uC904 4\uC640 8\uC5D0 \uAC00 \uC788\uC5C8\uC2B5\uB2C8\uB2E4.
        \n\n\uC774\uB300\uB85C \uAE30\uC219\uC0AC\uB85C \uAC04\uB2E4\uBA74 \uC9C0\uAC01\uC744
        \uD574 \uBC8C\uC810\uC744 \uBC1B\uC744 \uAC83\uC785\uB2C8\uB2E4. \n\n\uB2F9\uC2E0\uC740
        \uBB34\uC5C7\uC744 \uC5B4\uB5BB\uAC8C \uD558\uC2DC\uACA0\uC2B5\uB2C8\uAE4C?"
      selectLines:
      - selectLine: "\uBC8C\uC810\uC740 \uC8FD\uB294\uAC83\uB9CC\uD07C \uC2EB\uB2E4!
          \uC53B\uB294\uAC74 \uAC00\uC11C \uC53B\uC790 \uB2F9\uC7A5 \uB098\uAC00"
        selectType: 0
        needStatType: 0
        needStat: 0
        eventStoryID: 21
      - selectLine: "\uAE09\uD560\uC218\uB85D \uB3CC\uC544\uAC00\uB77C! \uADF8\uB798\uB3C4
          \uBC16\uC5D0 \uB098\uAC00\uB294\uAC70\uB2C8\uAE4C \uC53B\uACE0 \uB098\uAC00\uC790"
        selectType: 0
        needStatType: 0
        needStat: 0
        eventStoryID: 22
      - selectLine: "\uC5B4\uCC28\uD53C \uB2A6\uC5C8\uB294\uB370 \uC774\uB300\uB85C
          \uB0B4\uC77C \uB4F1\uAD50\uD558\uAE30 \uC704\uD55C \uC7A0\uC744 \uCCAD\uD558\uC790"
        selectType: 0
