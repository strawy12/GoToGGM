  - scenarioName: "\uD504\uB864\uB85C\uADF8"
    stories:
    - storyName: "\uBE44\uD0C0\uBBFC D \uCDA9\uC804 \uD558\uB294 \uC815\uBC30"
      mainStory: "\uB2F9\uC2E0\uC740 \uB208\uC5D0 \uAC15\uB82C\uD55C \uBE44\uD0C0\uBBFC
        D\uC744 \uBC1B\uC73C\uBA70 \uC7A0\uC5D0\uC11C \uAE68\uC5B4\uB0A9\uB2C8\uB2E4.\r\n\n\uC7A0\uC5D0\uC11C
        \uAE68\uC5B4\uB09C \uB2F9\uC2E0\uC740 \uB2F9\uC2E0\uC744 \uBC18\uAE30\uACE0
        \uC788\uB294 \uC54C\uB78C\uC2DC\uACC4\uAC00 \uC790\uC7A5\uAC00\uB97C \uBD88\uB7EC\uC8FC\uACE0
        \uC788\uC5C8\uC2B5\uB2C8\uB2E4.\r\n\r\n[\uBE60\uBE60\uBE60\uBE70\uBE70 \uBE60\uBC14\uBC14\uBE70!]
        (\u3145\u3142... \uC880 \uC77C\uC5B4\uB098\uB77C)\r\n\r\n\uC5B4\uB5BB\uAC8C
        \uD558\uC2DC\uACA0\uC2B5\uB2C8\uAE4C?"
      selectLines:
      - selectLine: "\uC54C\uB78C\uC2DC\uACC4\uB97C \uBD80\uC218\uACE0 \uC640\uC11C
          \uC794\uB2E4"
        selectType: 0
        needStatType: 0
        needStat: 0
        eventStoryID: 11
      - selectLine: "\uC790\uC7A5\uAC00\uB97C \uBD88\uB7EC\uC8FC\uB2C8 \uD589\uBCF5\uD558\uB124,
          \uACC4\uC18D \uC794\uB2E4"
