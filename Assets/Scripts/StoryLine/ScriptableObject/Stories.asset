        needStatType: 0
        needStat: 0
        eventStoryID: 12
      - selectLine: "\uC77C\uC5B4\uB098\uC11C \uC54C\uB78C\uC2DC\uACC4\uC5D0\uAC8C
          \uAD7F\uBAA8\uB2DD \uD0A4\uC2A4\uB97C \uD574\uC900\uB2E4."
        selectType: 0
